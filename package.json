{
  "name": "spotify-web-api-fetch",
  "version": "7.0.0",
  "homepage": "https://github.com/eligundry/spotify-web-api-fetch",
  "description": "A Node.js wrapper for Spotify's Web API",
  "main": "./dist/server.js",
  "browser": {
    "./dist/server.js": "./dist/client.js"
  },
  "author": "Michael Thelin",
  "contributors": [
    {
      "name": "Eli Gundry",
      "url": "https://github.com/eligundry"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eligundry/spotify-web-api-fetch.git"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "npm test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "precommit": "lint-staged",
    "prettier": "prettier . --write",
    "build": "tsc"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/spotify-api": "^0.0.22",
    "coveralls": "^3.1.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-resolve": "^29.6.2",
    "lint-staged": "^13.2.3",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "spotify",
    "echonest",
    "music",
    "api",
    "wrapper",
    "client",
    "web api"
  ],
  "files": [
    "./src",
    "./LICENSE",
    "./package.json",
    "./README.md",
    "./CHANGELOG.md"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always"
  }
}
